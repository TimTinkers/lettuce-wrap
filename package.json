  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "copy-webpack-plugin": "^5.0.0",
    "dts-bundle": "^0.7.3",
    "prettier": "^1.16.4",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@types/node": "^11.9.5"
  }
}
