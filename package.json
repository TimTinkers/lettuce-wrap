{
  "name": "lettuce-wrap",
  "version": "0.1.0",
  "description": "JS wrapper for ethminer",
  "repository": "https://github.com/SaladTechnologies/lettuce-wrap.git",
  "author": "Salad Technologies",
  "keywords": [
    "ethminer",
    "crypto",
    "mining",
    "ethereum"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "dev": "ts-node example/example.ts",
    "test:": "",
    "build": "webpack",
